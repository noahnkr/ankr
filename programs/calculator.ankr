output("Enter first number");
var a = input();

output("Enter second number");
var b = input();

output("Enter operator (+, -, *, /)");
var op = input();

if (op == "+") {
 output("Result: " + (a + b));
} else if (op == "-") {
 output("Result: " + (a - b));
} else if (op == "*") {
 output("Result: " + (a * b));
} else if (op == "/") {
 if (b == 0) {
   output("Cannot divide by zero");
 } else {
  output("Result: " + (a / b));
 }
} else {
 output("Unknown operator");
}

