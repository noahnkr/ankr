var guesses = 5;
var guess_num =  26;
var guess = 0;

while (guess != guess_num && guesses > 0) {
  output("Guess a number. Guesses Remaining: " + guesses);
  guess = input();

  if (guess > guess_num) {
    output("Too High!");
    guesses--;

  } else if (guess < guess_num) {
    output("Too Low!");
    guesses--;

  } else {
    output("You Win! The number was: " + guess_num);
  }

}

if (guesses == 0) {
  output("You Lose! The number was: " + guess_num);
}
